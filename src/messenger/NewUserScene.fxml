<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="messenger.NewUserSceneController">
   <children>
      <Button fx:id="continueButton" layoutX="268.0" layoutY="257.0" mnemonicParsing="false" onAction="#continueButtonPress" text="Continue" />
      <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="162.0" promptText="password" />
      <PasswordField fx:id="confirmPasswordField" layoutX="226.0" layoutY="209.0" promptText="confirm password" />
      <Label layoutX="162.0" layoutY="166.0" text="Password" />
      <Label layoutX="116.0" layoutY="213.0" text="Confirm Password" />
      <Label layoutX="181.0" layoutY="119.0" text="Email" />
      <TextField fx:id="emailField" layoutX="226.0" layoutY="115.0" promptText="example@email.com" />
      <Label layoutX="168.0" layoutY="45.0" text="New User Account Registration">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Hyperlink fx:id="signInLink" layoutX="326.0" layoutY="305.0" onAction="#signInAction" text="Sign in" underline="true">
         <font>
            <Font size="14.0" />
         </font></Hyperlink>
      <Label layoutX="168.0" layoutY="309.0" text="Already have an account?">
         <font>
            <Font size="14.0" />
         </font></Label>
   </children>
</AnchorPane>
